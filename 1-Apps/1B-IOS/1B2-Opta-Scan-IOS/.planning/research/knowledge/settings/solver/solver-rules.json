{
  "$schema": "../schema/settings-schema.json",
  "domain": "solver-rules",
  "subDomain": "configuration-calculator",
  "lastUpdated": "2026-01-18",
  "lastVerified": "2026-01-18",
  "confidence": "high",
  "version": "1.0.0",
  "description": "Solver rules for Phase 47 Configuration Calculator. Defines constraint rules, optimization rules, goal-specific rules, platform rules, and evaluation rules for computing optimal setting combinations.",
  "sources": [
    {
      "name": "Phase 43 Settings Interaction Engine",
      "type": "derived",
      "date": "2026-01-18"
    },
    {
      "name": "Gemini Deep Research - Apple Silicon Gaming",
      "type": "gemini-research",
      "date": "2026-01-18"
    },
    {
      "name": "Apple Metal Performance Guidelines",
      "type": "official-docs",
      "url": "https://developer.apple.com/documentation/metal",
      "date": "2025"
    }
  ],
  "rules": [
    {
      "id": "constraint-upscaler-mutual-exclusion",
      "type": "constraint",
      "category": "must_not_conflict",
      "name": "Upscaler Mutual Exclusion",
      "description": "Only one upscaling technology can be active at a time",
      "condition": {
        "type": "and",
        "clauses": [
          { "setting": "upscaling:dlss", "operator": "eq", "value": "on" },
          { "setting": "upscaling:fsr", "operator": "eq", "value": "on" }
        ]
      },
      "action": {
        "type": "reject",
        "message": "Cannot enable multiple upscaling technologies simultaneously",
        "resolution": "disable_one",
        "prefer": "dlss"
      },
      "priority": 10,
      "source": "conflicts/resolution-upscaling.json"
    },
    {
      "id": "constraint-upscaler-native-resolution",
      "type": "constraint",
      "category": "must_not_conflict",
      "name": "Upscaler vs Native Resolution",
      "description": "Upscaling cannot be used with 100% render scale",
      "condition": {
        "type": "and",
        "clauses": [
          { "setting": "upscaling:any", "operator": "eq", "value": "on" },
          { "setting": "graphics:render-scale", "operator": "eq", "value": "100" }
        ]
      },
      "action": {
        "type": "adjust",
        "message": "Upscaling requires render scale below 100%",
        "adjustment": { "setting": "graphics:render-scale", "value": "75" }
      },
      "priority": 9,
      "source": "conflicts/resolution-upscaling.json"
    },
    {
      "id": "constraint-taa-dlss-redundant",
      "type": "constraint",
      "category": "must_not_conflict",
      "name": "TAA with DLSS Redundant",
      "description": "TAA is redundant when DLSS is enabled",
      "condition": {
        "type": "and",
        "clauses": [
          { "setting": "graphics:aa", "operator": "eq", "value": "taa" },
          { "setting": "upscaling:dlss", "operator": "eq", "value": "on" }
        ]
      },
      "action": {
        "type": "adjust",
        "message": "DLSS includes temporal anti-aliasing; disabling separate TAA",
        "adjustment": { "setting": "graphics:aa", "value": "off" }
      },
      "priority": 7,
      "source": "conflicts/resolution-upscaling.json"
    },
    {
      "id": "constraint-vsync-vrr-conflict",
      "type": "constraint",
      "category": "must_not_conflict",
      "name": "V-Sync with VRR Conflict",
      "description": "V-Sync adds latency when VRR is available",
      "condition": {
        "type": "and",
        "clauses": [
          { "setting": "display:vsync", "operator": "eq", "value": "on" },
          { "setting": "display:vrr", "operator": "eq", "value": "on" }
        ]
      },
      "action": {
        "type": "recommend",
        "message": "VRR provides tear-free gaming without V-Sync latency",
        "recommendation": { "setting": "display:vsync", "value": "off" }
      },
      "priority": 6,
      "source": "conflicts/resolution-upscaling.json"
    },
    {
      "id": "constraint-rt-hardware-required",
      "type": "constraint",
      "category": "hardware_capability",
      "name": "RT Hardware Required",
      "description": "Ray tracing requires M3+ hardware",
      "condition": {
        "type": "and",
        "clauses": [
          { "setting": "rt:any", "operator": "eq", "value": "on" },
          { "hardware": "chip_generation", "operator": "lt", "value": "M3" }
        ]
      },
      "action": {
        "type": "reject",
        "message": "Hardware ray tracing requires M3 or newer Apple Silicon",
        "resolution": "disable_rt"
      },
      "priority": 10,
      "source": "tradeoffs/ray-tracing.json"
    },
    {
      "id": "constraint-metal3-required-for-metalfx-temporal",
      "type": "constraint",
      "category": "hardware_capability",
      "name": "Metal 3 Required for MetalFX Temporal",
      "description": "MetalFX Temporal requires Metal 3 API",
      "condition": {
        "type": "and",
        "clauses": [
          { "setting": "upscaling:metalfx", "operator": "eq", "value": "temporal" },
          { "hardware": "metal_version", "operator": "lt", "value": "3" }
        ]
      },
      "action": {
        "type": "adjust",
        "message": "MetalFX Temporal requires Metal 3; falling back to Spatial",
        "adjustment": { "setting": "upscaling:metalfx", "value": "spatial" }
      },
      "priority": 9,
      "source": "tradeoffs/apple-silicon.json"
    },
    {
      "id": "constraint-memory-budget-8gb",
      "type": "constraint",
      "category": "memory_budget",
      "name": "8GB Memory Budget",
      "description": "8GB systems must limit texture quality",
      "condition": {
        "type": "and",
        "clauses": [
          { "hardware": "unified_memory", "operator": "lte", "value": 8 },
          { "setting": "graphics:textures", "operator": "in", "value": ["high", "ultra"] }
        ]
      },
      "action": {
        "type": "adjust",
        "message": "8GB memory requires Medium or lower textures to avoid memory pressure",
        "adjustment": { "setting": "graphics:textures", "value": "medium" }
      },
      "priority": 10,
      "source": "tradeoffs/apple-silicon.json"
    },
    {
      "id": "constraint-memory-budget-rt",
      "type": "constraint",
      "category": "memory_budget",
      "name": "RT Memory Budget",
      "description": "Multiple RT features require 16GB+ memory",
      "condition": {
        "type": "and",
        "clauses": [
          { "hardware": "unified_memory", "operator": "lt", "value": 16 },
          { "setting": "rt:reflections", "operator": "eq", "value": "on" },
          { "setting": "rt:shadows", "operator": "eq", "value": "on" }
        ]
      },
      "action": {
        "type": "warn",
        "message": "Multiple RT features may cause memory pressure on systems under 16GB",
        "recommendation": "Enable only one RT feature or upgrade memory"
      },
      "priority": 8,
      "source": "tradeoffs/ray-tracing.json"
    },
    {
      "id": "constraint-rt-gi-memory",
      "type": "constraint",
      "category": "memory_budget",
      "name": "RT GI Memory Requirement",
      "description": "RT Global Illumination requires significant VRAM",
      "condition": {
        "type": "and",
        "clauses": [
          { "setting": "rt:global-illumination", "operator": "eq", "value": "on" },
          { "hardware": "unified_memory", "operator": "lt", "value": 32 }
        ]
      },
      "action": {
        "type": "warn",
        "message": "RT Global Illumination works best with 32GB+ memory",
        "recommendation": "Consider disabling RT GI on systems under 32GB"
      },
      "priority": 7,
      "source": "tradeoffs/ray-tracing.json"
    },
    {
      "id": "dependency-rt-shadows-quality",
      "type": "constraint",
      "category": "must_satisfy_dependency",
      "name": "RT Requires Shadow Quality",
      "description": "RT reflections work better with at least medium shadow quality",
      "condition": {
        "type": "and",
        "clauses": [
          { "setting": "rt:reflections", "operator": "eq", "value": "on" },
          { "setting": "graphics:shadows", "operator": "eq", "value": "low" }
        ]
      },
      "action": {
        "type": "recommend",
        "message": "RT reflections benefit from medium+ shadow quality for accurate reflection data",
        "recommendation": { "setting": "graphics:shadows", "value": "medium" }
      },
      "priority": 5,
      "source": "tradeoffs/ray-tracing.json"
    },
    {
      "id": "optimization-prefer-synergy-rt-bvh",
      "type": "optimization",
      "category": "prefer_synergy",
      "name": "RT BVH Sharing Synergy",
      "description": "Multiple RT features share BVH for efficiency",
      "condition": {
        "type": "and",
        "clauses": [
          { "setting": "rt:reflections", "operator": "eq", "value": "on" }
        ]
      },
      "action": {
        "type": "suggest",
        "message": "RT Reflections and RT Shadows share BVH - enabling both is more efficient than expected",
        "suggestion": { "setting": "rt:shadows", "value": "on" }
      },
      "priority": 4,
      "source": "synergies/optimal-combinations.json"
    },
    {
      "id": "optimization-prefer-synergy-taa-sharpening",
      "type": "optimization",
      "category": "prefer_synergy",
      "name": "TAA + Sharpening Synergy",
      "description": "TAA with sharpening produces cleaner image",
      "condition": {
        "type": "and",
        "clauses": [
          { "setting": "graphics:aa", "operator": "eq", "value": "taa" },
          { "setting": "graphics:sharpening", "operator": "eq", "value": "off" }
        ]
      },
      "action": {
        "type": "suggest",
        "message": "TAA benefits from sharpening to counter blur",
        "suggestion": { "setting": "graphics:sharpening", "value": "50" }
      },
      "priority": 3,
      "source": "synergies/optimal-combinations.json"
    },
    {
      "id": "optimization-diminishing-returns-shadows",
      "type": "optimization",
      "category": "diminishing_returns",
      "name": "Shadow Quality Diminishing Returns",
      "description": "Ultra shadows have minimal visual benefit over High",
      "condition": {
        "type": "and",
        "clauses": [
          { "setting": "graphics:shadows", "operator": "eq", "value": "ultra" },
          { "goal": "type", "operator": "neq", "value": "quality" }
        ]
      },
      "action": {
        "type": "recommend",
        "message": "Ultra shadows offer minimal improvement over High with significant GPU cost",
        "recommendation": { "setting": "graphics:shadows", "value": "high" }
      },
      "priority": 4,
      "source": "synergies/optimal-combinations.json"
    },
    {
      "id": "optimization-diminishing-returns-aa",
      "type": "optimization",
      "category": "diminishing_returns",
      "name": "AA at 4K Diminishing Returns",
      "description": "Anti-aliasing less important at 4K resolution",
      "condition": {
        "type": "and",
        "clauses": [
          { "setting": "graphics:resolution", "operator": "gte", "value": "3840x2160" },
          { "setting": "graphics:aa", "operator": "in", "value": ["msaa", "smaa"] }
        ]
      },
      "action": {
        "type": "recommend",
        "message": "At 4K, complex AA provides diminishing returns - consider TAA or FXAA",
        "recommendation": { "setting": "graphics:aa", "value": "taa" }
      },
      "priority": 3,
      "source": "synergies/optimal-combinations.json"
    },
    {
      "id": "optimization-bottleneck-cpu-bound",
      "type": "optimization",
      "category": "bottleneck_detection",
      "name": "CPU Bottleneck Detection",
      "description": "Detect when CPU limits GPU performance",
      "condition": {
        "type": "and",
        "clauses": [
          { "telemetry": "cpu_utilization", "operator": "gt", "value": 90 },
          { "telemetry": "gpu_utilization", "operator": "lt", "value": 70 }
        ]
      },
      "action": {
        "type": "diagnose",
        "message": "CPU is bottlenecking GPU - reducing draw calls may help",
        "recommendations": [
          { "setting": "graphics:draw-distance", "value": "medium" },
          { "setting": "graphics:crowd-density", "value": "low" }
        ]
      },
      "priority": 8,
      "source": "derived"
    },
    {
      "id": "optimization-bottleneck-vram-pressure",
      "type": "optimization",
      "category": "bottleneck_detection",
      "name": "VRAM Pressure Detection",
      "description": "Detect when memory pressure is causing issues",
      "condition": {
        "type": "and",
        "clauses": [
          { "telemetry": "memory_pressure", "operator": "eq", "value": "critical" }
        ]
      },
      "action": {
        "type": "urgent",
        "message": "Critical memory pressure detected - immediate settings reduction needed",
        "adjustments": [
          { "setting": "graphics:textures", "value": "-1_tier" },
          { "setting": "graphics:render-scale", "value": "-10_percent" }
        ]
      },
      "priority": 10,
      "source": "tradeoffs/apple-silicon.json"
    },
    {
      "id": "optimization-balanced-quality-tier",
      "type": "optimization",
      "category": "balanced_quality",
      "name": "Balanced Quality Tiers",
      "description": "Related settings should be at similar quality tiers",
      "condition": {
        "type": "and",
        "clauses": [
          { "setting": "graphics:textures", "operator": "eq", "value": "ultra" },
          { "setting": "graphics:shadows", "operator": "eq", "value": "low" }
        ]
      },
      "action": {
        "type": "recommend",
        "message": "Mismatched quality tiers - consider balancing textures and shadows",
        "recommendation": "Set both to High for balanced visual quality"
      },
      "priority": 2,
      "source": "synergies/optimal-combinations.json"
    },
    {
      "id": "goal-maxfps-disable-expensive",
      "type": "goal",
      "category": "maxFps_priority",
      "name": "Max FPS Priority - Disable Expensive Effects",
      "description": "For max FPS goal, disable expensive effects first",
      "condition": {
        "type": "and",
        "clauses": [
          { "goal": "type", "operator": "eq", "value": "maxFps" }
        ]
      },
      "action": {
        "type": "apply_preset",
        "order": [
          { "setting": "rt:any", "value": "off", "impact": 40 },
          { "setting": "graphics:motion-blur", "value": "off", "impact": 8 },
          { "setting": "graphics:depth-of-field", "value": "off", "impact": 5 },
          { "setting": "graphics:shadows", "value": "low", "impact": 15 },
          { "setting": "graphics:post-processing", "value": "low", "impact": 10 },
          { "setting": "graphics:aa", "value": "fxaa", "impact": 5 }
        ],
        "stopWhen": "target_fps_achieved"
      },
      "priority": 9,
      "source": "hardware-recommendations.json"
    },
    {
      "id": "goal-quality-enable-expensive",
      "type": "goal",
      "category": "quality_priority",
      "name": "Quality Priority - Enable Expensive Effects",
      "description": "For quality goal, enable expensive effects if budget allows",
      "condition": {
        "type": "and",
        "clauses": [
          { "goal": "type", "operator": "eq", "value": "quality" },
          { "telemetry": "fps", "operator": "gt", "value": 40 }
        ]
      },
      "action": {
        "type": "apply_enhancements",
        "order": [
          { "setting": "graphics:textures", "value": "ultra", "cost": 5 },
          { "setting": "graphics:shadows", "value": "ultra", "cost": 10 },
          { "setting": "rt:reflections", "value": "on", "cost": 20 },
          { "setting": "rt:shadows", "value": "on", "cost": 15 },
          { "setting": "graphics:post-processing", "value": "ultra", "cost": 8 }
        ],
        "stopWhen": "fps_below_30"
      },
      "priority": 9,
      "source": "hardware-recommendations.json"
    },
    {
      "id": "goal-battery-reduce-load",
      "type": "goal",
      "category": "battery_priority",
      "name": "Battery Priority - Reduce System Load",
      "description": "For battery goal, systematically reduce GPU and CPU load",
      "condition": {
        "type": "and",
        "clauses": [
          { "goal": "type", "operator": "eq", "value": "battery" }
        ]
      },
      "action": {
        "type": "apply_preset",
        "settings": [
          { "setting": "display:frame-cap", "value": "30" },
          { "setting": "display:refresh-rate", "value": "60" },
          { "setting": "rt:any", "value": "off" },
          { "setting": "graphics:render-scale", "value": "75" },
          { "setting": "graphics:quality-preset", "value": "medium" },
          { "setting": "power:low-power-mode", "value": "on" }
        ]
      },
      "priority": 9,
      "source": "hardware-recommendations.json"
    },
    {
      "id": "goal-competitive-minimize-latency",
      "type": "goal",
      "category": "competitive_priority",
      "name": "Competitive Priority - Minimize Latency",
      "description": "For competitive gaming, minimize input lag above all",
      "condition": {
        "type": "and",
        "clauses": [
          { "goal": "type", "operator": "eq", "value": "competitive" }
        ]
      },
      "action": {
        "type": "apply_preset",
        "settings": [
          { "setting": "display:vsync", "value": "off" },
          { "setting": "display:frame-cap", "value": "240" },
          { "setting": "graphics:motion-blur", "value": "off" },
          { "setting": "graphics:depth-of-field", "value": "off" },
          { "setting": "graphics:film-grain", "value": "off" },
          { "setting": "graphics:post-processing", "value": "off" },
          { "setting": "graphics:quality-preset", "value": "low" },
          { "setting": "metal:triple-buffering", "value": "off" }
        ]
      },
      "priority": 10,
      "source": "synergies/optimal-combinations.json"
    },
    {
      "id": "platform-macos-prefer-metalfx",
      "type": "platform",
      "category": "macos_prefer_metalfx",
      "name": "macOS Prefer MetalFX",
      "description": "On macOS, prefer MetalFX over other upscaling",
      "condition": {
        "type": "and",
        "clauses": [
          { "platform": "os", "operator": "eq", "value": "macos" },
          { "setting": "upscaling:any", "operator": "eq", "value": "on" }
        ]
      },
      "action": {
        "type": "prefer",
        "message": "MetalFX is optimized for Apple Silicon",
        "preference": { "setting": "upscaling:metalfx", "value": "temporal" }
      },
      "priority": 7,
      "source": "tradeoffs/apple-silicon.json"
    },
    {
      "id": "platform-macos-power-mode-aware",
      "type": "platform",
      "category": "macos_power_mode_aware",
      "name": "macOS Power Mode Awareness",
      "description": "Adjust recommendations based on power state",
      "condition": {
        "type": "and",
        "clauses": [
          { "platform": "os", "operator": "eq", "value": "macos" },
          { "platform": "power_source", "operator": "eq", "value": "battery" }
        ]
      },
      "action": {
        "type": "apply_modifier",
        "message": "Running on battery - applying power-conscious settings",
        "modifiers": [
          { "setting": "display:frame-cap", "modifier": "reduce_to_60" },
          { "setting": "display:refresh-rate", "modifier": "lock_to_60" },
          { "setting": "rt:any", "modifier": "disable" }
        ]
      },
      "priority": 8,
      "source": "tradeoffs/apple-silicon.json"
    },
    {
      "id": "platform-macos-uma-aware",
      "type": "platform",
      "category": "apple_silicon_uma",
      "name": "Apple Silicon UMA Awareness",
      "description": "Account for shared memory on Apple Silicon",
      "condition": {
        "type": "and",
        "clauses": [
          { "platform": "os", "operator": "eq", "value": "macos" },
          { "hardware": "architecture", "operator": "eq", "value": "arm64" }
        ]
      },
      "action": {
        "type": "adjust_calculation",
        "message": "Unified Memory Architecture - textures and system share same pool",
        "calculation": {
          "vram_available": "unified_memory * 0.7",
          "texture_budget": "unified_memory * 0.4",
          "system_reserve": "unified_memory * 0.3"
        }
      },
      "priority": 9,
      "source": "tradeoffs/apple-silicon.json"
    },
    {
      "id": "platform-macos-high-power-mode",
      "type": "platform",
      "category": "macos_high_power",
      "name": "High Power Mode Optimization",
      "description": "Enable higher settings when High Power Mode is available",
      "condition": {
        "type": "and",
        "clauses": [
          { "platform": "os", "operator": "eq", "value": "macos" },
          { "platform": "power_source", "operator": "eq", "value": "plugged_in" },
          { "hardware": "chip", "operator": "in", "value": ["M1 Pro", "M1 Max", "M2 Pro", "M2 Max", "M3 Pro", "M3 Max", "M4 Pro", "M4 Max"] }
        ]
      },
      "action": {
        "type": "recommend",
        "message": "High Power Mode available - enable for best gaming performance",
        "recommendation": { "setting": "power:high-power-mode", "value": "on" }
      },
      "priority": 6,
      "source": "tradeoffs/apple-silicon.json"
    },
    {
      "id": "platform-thermal-fanless",
      "type": "platform",
      "category": "thermal_management",
      "name": "Fanless Device Thermal Management",
      "description": "Limit settings on fanless MacBook Air",
      "condition": {
        "type": "and",
        "clauses": [
          { "hardware": "device", "operator": "contains", "value": "MacBook Air" }
        ]
      },
      "action": {
        "type": "limit",
        "message": "MacBook Air fanless design limits sustained performance",
        "limits": [
          { "setting": "graphics:quality-preset", "max": "medium" },
          { "setting": "display:frame-cap", "max": 60 }
        ]
      },
      "priority": 8,
      "source": "tradeoffs/apple-silicon.json"
    },
    {
      "id": "evaluation-impact-calculation",
      "type": "evaluation",
      "category": "impact_calculation",
      "name": "Impact Calculation Method",
      "description": "How to sum setting impacts",
      "condition": {
        "type": "always"
      },
      "action": {
        "type": "define_calculation",
        "method": "weighted_sum",
        "weights": {
          "performance": 1.0,
          "quality": 0.8,
          "power": 0.6,
          "vram": 0.9,
          "latency": 0.7
        },
        "diminishing_factor": 0.85,
        "notes": "Impacts compound with diminishing returns - third expensive effect costs less than sum"
      },
      "priority": 5,
      "source": "derived"
    },
    {
      "id": "evaluation-conflict-resolution",
      "type": "evaluation",
      "category": "conflict_resolution",
      "name": "Conflict Resolution Priority",
      "description": "How to resolve conflicting recommendations",
      "condition": {
        "type": "always"
      },
      "action": {
        "type": "define_priority",
        "order": [
          "hardware_capability",
          "memory_budget",
          "must_not_conflict",
          "goal_specific",
          "platform_specific",
          "optimization_synergy"
        ],
        "notes": "Hardware limits always win, then memory, then conflicts, then goals"
      },
      "priority": 10,
      "source": "derived"
    },
    {
      "id": "evaluation-uncertainty-handling",
      "type": "evaluation",
      "category": "uncertainty_handling",
      "name": "Low Confidence Handling",
      "description": "How to handle recommendations with low confidence",
      "condition": {
        "type": "and",
        "clauses": [
          { "rule": "confidence", "operator": "eq", "value": "low" }
        ]
      },
      "action": {
        "type": "mark_optional",
        "message": "This recommendation has limited data - consider testing",
        "presentation": "suggestion_not_default",
        "allow_override": true
      },
      "priority": 2,
      "source": "derived"
    },
    {
      "id": "evaluation-game-specific-override",
      "type": "evaluation",
      "category": "game_override",
      "name": "Game-Specific Override",
