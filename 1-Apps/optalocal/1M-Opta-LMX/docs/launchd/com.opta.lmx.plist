        <string>/Users/opta/venvs/mlx/bin:/usr/local/bin:/usr/bin:/bin</string>
        <key>HOME</key>
        <string>/Users/opta</string>
        <key>PYTHONUNBUFFERED</key>
        <string>1</string>
        <key>PYTHONPATH</key>
        <string>/Users/Shared/312/Opta/1-Apps/1M-Opta-LMX/src</string>
    </dict>

    <key>SoftResourceLimits</key>
    <dict>
        <key>NumberOfFiles</key>
        <integer>65536</integer>
    </dict>

    <key>ThrottleInterval</key>
    <integer>10</integer>

    <key>ProcessType</key>
    <string>Standard</string>

    <key>ExitTimeOut</key>
    <integer>120</integer>

    <key>AbandonProcessGroup</key>
    <false/>
</dict>
</plist>
