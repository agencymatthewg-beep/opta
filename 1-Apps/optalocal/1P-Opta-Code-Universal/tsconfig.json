{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "paths": {
      "@opta/protocol-shared": [
        "../1D-Opta-CLI-TS/packages/protocol-shared/src/index.ts"
      ],
      "@opta/protocol-shared/*": [
        "../1D-Opta-CLI-TS/packages/protocol-shared/src/*"
      ],
      "@opta/daemon-client": [
        "../1D-Opta-CLI-TS/packages/daemon-client/src/index.ts"
      ],
      "@opta/daemon-client/*": [
        "../1D-Opta-CLI-TS/packages/daemon-client/src/*"
      ]
    },
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "types": ["vite/client"]
  },
  "include": ["src", "vite.config.ts"]
}
