{
  "name": "@opta/daemon-client",
  "version": "0.0.0",
  "private": true,
  "description": "Typed HTTP and WebSocket client wrappers for the Opta daemon API",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "default": "./src/types.ts"
    },
    "./http-client": {
      "types": "./src/http-client.ts",
      "default": "./src/http-client.ts"
    },
    "./ws-client": {
      "types": "./src/ws-client.ts",
      "default": "./src/ws-client.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@opta/protocol-shared": "file:../protocol-shared",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1",
    "typescript": "^5.7.0"
  }
}
