{
  "name": "@opta/protocol-shared",
  "version": "0.0.0",
  "private": true,
  "description": "Stable protocol v3 entrypoints shared across Opta packages",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "default": "./src/types.ts"
    },
    "./http": {
      "types": "./src/http.ts",
      "default": "./src/http.ts"
    },
    "./ws": {
      "types": "./src/ws.ts",
      "default": "./src/ws.ts"
    },
    "./events": {
      "types": "./src/events.ts",
      "default": "./src/events.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  }
}
